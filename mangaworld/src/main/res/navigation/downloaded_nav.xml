<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/downloaded_nav"
    app:startDestination="@id/downloadViewerFragment">

    <fragment
        android:id="@+id/downloadViewerFragment"
        android:name="com.programmersbox.mangaworld.DownloadViewerFragment"
        android:label="fragment_download_viewer"
        tools:layout="@layout/fragment_download_viewer">
        <action
            android:id="@+id/action_downloadViewerFragment_self"
            app:destination="@id/downloadViewerFragment" />
        <argument
            android:name="pathname"
            app:argType="java.io.File"
            app:nullable="true"
            android:defaultValue="@null" />
    </fragment>
</navigation>